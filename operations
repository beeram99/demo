public class index {
    public static void main(String arg[]) {
        int n1 = 4;
        int n2 = 886;
        addition add = new addition();
        int N = add.sum(n1, n2);
        System.out.println(N);

    }

}

class addition {
    public int sum(int n1, int n2) {
        int N = n1 + n2;
        return N;
    }

}

class substraction {
    public int sub(int a, int b) {
        int z = a - b;
        return z;
    }

}
